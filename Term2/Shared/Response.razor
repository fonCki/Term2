@using Term2.Services
<main class="main">
    @if (Resp is not null) {
        switch (Resp!.Name) {
            case "about" :  <Photo/>; <p>@ms</p>; break;
            case "banner":  <Bannerrrrr/>; <p>@ms</p> break;
            default: <p>@ms</p> break;
        }
    }
</main>

@code {

    [Parameter]
    public Command Resp { get; set; }
    MarkupString ms => (MarkupString)Resp!.Response;
}